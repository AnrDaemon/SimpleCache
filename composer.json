{
    "name": "anrdaemon/simple-cache",
    "description": "A clollection of PSR-16 (SimpleCache) compatible classes (NullCache, ArrayCache, ...)",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Andrey Repin",
            "email": "anrdaemon@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "psr/simple-cache": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AnrDaemon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnrDaemon\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
